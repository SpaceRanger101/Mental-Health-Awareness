<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scrollView2"
    android:layout_width="419dp"
    android:layout_height="738dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:ignore="SpeakableTextPresentCheck">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/lavenderbkg"
        tools:context=".FEMALES">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="398dp"
            android:layout_height="701dp"
            android:layout_marginBottom="52dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/lavenderbkg" />

        <TextView
            android:id="@+id/article_heading2"
            android:layout_width="388dp"
            android:layout_height="61dp"
            android:background="@color/lavender"
            android:padding="14dp"
            android:text="@string/male_module"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/anxiety"
            android:layout_width="346dp"
            android:layout_height="68dp"
            android:shadowColor="#A8A8A8"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="5"
            android:text="@string/anxiety"
            android:textColor="#ADFFFFFF"
            android:textSize="20sp"
            app:backgroundTint="#541B1B1B"
            app:layout_constraintBottom_toTopOf="@+id/angry_btn2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.48"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/article_heading2"
            app:layout_constraintVertical_bias="0.444" />

        <Button
            android:id="@+id/angry_btn2"
            android:layout_width="346dp"
            android:layout_height="68dp"
            android:shadowColor="#A8A8A8"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="5"
            android:text="@string/strss"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:backgroundTint="#541B1B1B"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.48"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.24" />

        <Button
            android:id="@+id/angry_btn3"
            android:layout_width="346dp"
            android:layout_height="68dp"
            android:shadowColor="#A8A8A8"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="5"
            android:text="@string/depp"
            android:textColor="#ADFFFFFF"
            android:textSize="20sp"
            app:backgroundTint="#541B1B1B"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/angry_btn2"
            app:layout_constraintVertical_bias="0.036" />

        <Button
            android:id="@+id/angry_btn4"
            android:layout_width="346dp"
            android:layout_height="68dp"
            android:shadowColor="#A8A8A8"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="5"
            android:text="@string/ptsd"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:backgroundTint="#541B1B1B"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/angry_btn3"
            app:layout_constraintVertical_bias="0.022" />

        <Button
            android:id="@+id/angry_btn6"
            android:layout_width="346dp"
            android:layout_height="68dp"
            android:shadowColor="#A8A8A8"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="5"
            android:text="@string/suiPrev"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:backgroundTint="#541B1B1B"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.493"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/angry_btn"
            app:layout_constraintVertical_bias="0.092" />

        <Button
            android:id="@+id/angry_btn7"
            android:layout_width="346dp"
            android:layout_height="68dp"
            android:shadowColor="#A8A8A8"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="5"
            android:text="@string/PHYSICAL"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:backgroundTint="#541B1B1B"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.48"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/angry_btn6"
            app:layout_constraintVertical_bias="0.155" />

        <Button
            android:id="@+id/angry_btn"
            android:layout_width="346dp"
            android:layout_height="68dp"
            android:shadowColor="#A8A8A8"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="5"
            android:text="@string/ocd"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:backgroundTint="#541B1B1B"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.48"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/angry_btn5"
            app:layout_constraintVertical_bias="0.058" />

        <Button
            android:id="@+id/angry_btn5"
            android:layout_width="346dp"
            android:layout_height="68dp"
            android:shadowColor="#A8A8A8"
            android:shadowDx="0"
            android:shadowDy="0"
            android:shadowRadius="5"
            android:text="@string/bipolar"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:backgroundTint="#541B1B1B"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/angry_btn4"
            app:layout_constraintVertical_bias="0.035" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
